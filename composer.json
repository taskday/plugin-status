{
    "name": "taskday/plugin-status",
    "description": "",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "taskday/framework": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Performing\\Taskday\\Status\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Performing\\Taskday\\Status\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Performing\\Taskday\\Status\\StatusServiceProvider"
            ]
        }
    }
}
